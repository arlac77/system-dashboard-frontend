{
  "name": "@konsumation/frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "svelte web frontend for konsum",
  "keywords": [
    "hook-ci",
    "konsum",
    "svelte",
    "template",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Konstantin Karpman"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "start": "rollup -c -w rollup.config.mjs && vite && rollup -c -w",
    "startkonsum": "node_modules/konsum/src/konsum-cli.mjs -c tests/config start",
    "startfrontend": "npm run dev",
    "test": "npm run test:ava && npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 5000 --app \"rollup -c  -w\"",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "lint": "npm run lint:css",
    "lint:css": "stylelint ./src/*.css",
    "cafeHead": "testcafe chrome tests/cafe/deleteValues.js -s build/test",
    "dev": "vite",
    "serve": "vite preview"
  },
  "dependencies": {
    "@kronos-integration/svelte-components": "^2.4.69",
    "aggregation-repository-provider": "^5.1.1",
    "bitbucket-repository-provider": "^4.1.6",
    "buffer": "^6.0.1",
    "content-entry": "^5.0.0",
    "gitea-repository-provider": "^2.1.31",
    "github-repository-provider": "^7.26.22",
    "imask": "^6.4.2",
    "layercake": "^6.1.0",
    "mf-styling": "^1.0.13",
    "microtip": "^0.2.2",
    "reader-line-iterator": "^1.1.2",
    "repository-provider": "^28.3.4",
    "streamsaver": "^2.0.6",
    "svelte-command": "^1.1.9",
    "svelte-common": "^4.4.33",
    "svelte-entitlement": "^1.2.19",
    "svelte-guard-history-router": "^4.6.5",
    "svelte-log-view": "^4.2.9",
    "svelte-repository-provider": "^1.7.67",
    "svelte-session-manager": "^2.0.10",
    "svelte-websocket-store": "^1.1.23",
    "the-new-css-reset": "^1.5.1"
  },
  "devDependencies": {
    "@konsumation/konsum": "github:konsumation/konsum",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-virtual": "^2.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.42",
    "ava": "^4.2.0",
    "konsum": "github:konsumation/konsum",
    "npm-pkgbuild": "^10.0.1",
    "rollup": "^2.70.2",
    "rollup-plugin-consts": "^1.1.0",
    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.2",
    "stylelint": "^14.8.0",
    "stylelint-config-standard": "^25.0.0",
    "svelte": "^3.47.0",
    "svql": "0.0.35",
    "testcafe": "^1.18.6",
    "vite": "^2.9.6"
  },
  "optionalDependencies": {
    "mf-hosting": "github:arlac77/mf-hosting"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/system-dashboard-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/system-dashboard-frontend/issues"
  },
  "homepage": "https://github.com/arlac77/system-dashboard-frontend#readme",
  "config": {
    "api": "/services/system-dashboard/api",
    "api_ws": "wss://mfelten.dynv6.net/services/system-dashboard/api",
    "base": "/services/system-dashboard",
    "graphQl": "/services/ci/api/graphql",
    "journal.endpoint": "/services/journal",
    "proxyTarget": "https://mfelten.dynv6.net/services/journal/",
    "title": "System Dashboard"
  },
  "pkgbuild": {
    "content": {
      "${install.dir}": {
        "base": "public"
      },
      "${nginx.sites.dir}${name}.conf": "pkg/nginx.conf"
    },
    "hooks": "pkg/hooks.sh",
    "groups": "monitoring",
    "depends": {
      "nginx-mainline": ">=1.21.4",
      "system-dashboard": ">=1.25.88",
      "entitlement-provider": ">=2.9.69",
      "hook-ci": ">=1.56.0",
      "konsum": ">=5.0.39"
    },
    "http.api.path": "/services/konsum/api",
    "http.base.path": "/services/konsum",
    "install.dir": "${install.base.dir}system-dashboard/frontend/",
    "name": "konsum-frontend-svelte"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild"
        }
      ],
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --verbose"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "template": {
    "inheritFrom": [
      "alrac77/template-systemd",
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-app"
    ]
  }
}
